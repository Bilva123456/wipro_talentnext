package java_fundamentals;

public class Question6 {
 public static void main(String[]args) {
	if(args.length == 0) {
		System.out.println("No value");
	 }else {
		 for(int i=1;i<args.length;i++) {
			 System.out.println(args[i]);
			 if(i<args.length-1) {
				 System.out.print(",");
			 }
		 }
	}
 }
}
